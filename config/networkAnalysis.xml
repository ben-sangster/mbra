            tileSize="256" 
            minZoom="0" 
            maxZoom="17"/>
         <map-adapter 
            name="Cloudmade-2" 
            type="tile" 
            server="tile.cloudmade.com" 
            path="/742140f4b12f5326be6b4c4e584c6528/2/256/%1/%2/%3.png" 
            tileSize="256" 
            minZoom="0" 
            maxZoom="17"/>
         <map-adapter 
            name="Cloudmade-3" 
            type="tile" 
            server="tile.cloudmade.com" 
            path="/742140f4b12f5326be6b4c4e584c6528/998/256/%1/%2/%3.png" 
            tileSize="256" 
            minZoom="0" 
            maxZoom="17"/>
         <map-adapter 
            name="Cloudmade-4" 
            type="tile" 
            server="tile.cloudmade.com" 
            path="/742140f4b12f5326be6b4c4e584c6528/4/256/%1/%2/%3.png" 
            tileSize="256" 
            minZoom="0" 
            maxZoom="17"/>
         <map-adapter 
            name="Cloudmade-5" 
            type="tile" 
            server="tile.cloudmade.com" 
            path="/742140f4b12f5326be6b4c4e584c6528/3417/256/%1/%2/%3.png" 
