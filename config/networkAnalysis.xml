<?xml version="1.0" encoding="utf-8"?>
<dmz>
   <plugin-list>
      <plugin name="dmzQtModuleMapBasic" unique="NAMap"/>
      <!-- <plugin name="dmzQtPluginMapZoomPan" unique="NAMapZoom"/> -->
      <plugin name="dmzQtPluginRenderPickMap" unique="NARenderPickMap"/>
      <plugin name="dmzQtPluginCanvasMap" unique="NACanvasMap"/>
      <plugin name="dmzQtModuleCanvasBasic" unique="NACanvas"/>
      <plugin name="dmzQtPluginCanvasZoomPanMap" unique="NACanvasMapZoom"/>
      <plugin name="dmzQtPluginCanvasObject" unique="NACanvasObject"/>
      <plugin name="dmzQtPluginCanvasLink" unique="NACanvasLink"/>
      <plugin name="dmzQtPluginCanvasLayer" unique="NACanvasLayer"/>
      <plugin name="dmzQtPluginRenderPickCanvas" unique="NARenderPickCanvas"/>
      <plugin name="dmzQtPluginMapArchive" unique="NAMapArchive"/>
      <plugin name="dmzQtPluginMapProperties"/>
      <plugin name="dmzQtPluginButtonToChannel"/>
      <!-- <plugin name="dmzQtPluginCanvasArchive" unique="NACanvasArchive"/> -->
      <!-- <plugin name="dmzQtPluginRenderPickCanvas" unique="NARenderPick"/> -->
      <plugin name="dmzMBRAPluginProperties" scope="na-properties"/>
      <plugin name="dmzMBRAPluginCalculate"/>
      <plugin name="dmzMBRAPluginNodeTable"/>
      <plugin name="dmzQtPluginCanvasAutoLink"/>
      <plugin name="dmzQtPluginGraph"/>
      <plugin name="dmzQtPluginTabWidget" unique="NABottomTab"/>
      <plugin name="dmzQtPluginMapObjectPosition"/>
   </plugin-list>
   <NABottomTab>
      <minimum-size width="300" height="250"/>
      <widget name="dmzMBRAPluginNodeTable" title="Network Table Data"/>
      <widget name="dmzQtPluginGraph" title="Degree Graph"/>
   </NABottomTab>
   <NACanvas>
      <canvas opengl="false" antialiasing="true" textantialiasing="true"/>
      <channel name="NetworkAnalysisChannel"/>
      <scene>
         <min x="-500000" y="-500000"/>
         <max x ="500000" y="500000"/>
         <background transparent="true" />
      </scene>
   </NACanvas>
   <NAMap>
      <!-- <channel name="NetworkAnalysisChannel"/>       -->
      <tileMapAdapter url="tile.cloudmade.com" path="/742140f4b12f5326be6b4c4e584c6528/2/256/%1/%2/%3.png"/>
      <!-- <tileMapAdapter url="tile.openstreetmap.org" path="/%1/%2/%3.png" /> -->
      <!-- <zoom min="17" max="0" />
      <tileMapAdapter url="mt2.google.com" path="/mt?n=404&amp;x=%2&amp;y=%3&amp;zoom=%1" /> -->
      <!-- <tileMapAdapter url="172.20.90.188" path="/cgi-bin/tilecache.cgi/1.0.0/osm/%1/%2/%3.png" /> -->
      <!-- <tileMapAdapter url="172.20.90.188" path="/tilecache/tilecache.py/1.0.0/osm/%1/%2/%3.png" /> -->

      <startCoordinate latitude="36.5973550921921" longitude="-121.876788139343"/>
   </NAMap>
   <NACanvasMap>
      <module>
         <canvas name="NACanvas"/>
         <map name="NAMap"/>
      </module>
      <canvas name="NACanvas"/>
      <map name="NAMap"/>
   </NACanvasMap>
   <NACanvasMapZoom>
      <scrollDelta value="10"/>
      <slidder min="0" max="17"/>
      <zoom min="0" max="17" step="1" default="6"/>
      <module>
         <canvas name="NACanvas"/>
         <map name="NAMap"/>
      </module>
      <!-- <widget>
         <stylesheet file="stylesheets:FileButton.qss"/>
      </widget> -->
      <zoomIn>
         <button>
            <iconSize width="18" height="18"/>
         </button>
         <action text="Zoom In" statusTip="Zoom In">
            <icon>
               <image file="images:Button_ZoomIn.png"/>
            </icon>
         </action>
      </zoomIn>
      <zoomOut>
         <button>
            <iconSize width="18" height="18"/>
         </button>
         <action text="Zoom Out" statusTip="Zoom Out">
            <icon>
               <image file="images:Button_ZoomOut.png"/>
            </icon>
         </action>
      </zoomOut>
      <zoomAll>
         <button>
            <iconSize width="18" height="18"/>
         </button>
         <action text="Zoom Extents" statusTip="Zoom Extents">
            <icon>
               <image file="images:Button_ArrowsZoomExtents.png"/>
            </icon>
         </action>
      </zoomAll>
      <panLeft>
         <button>
            <iconSize width="18" height="18"/>
         </button>
         <action text="Pan Left" statusTip="Pan Left">
            <icon>
               <image file="images:Button_ArrowLft.png"/>
            </icon>
         </action>
      </panLeft>
      <panRight>
         <button>
            <iconSize width="18" height="18"/>
         </button>
         <action text="Pan Right" statusTip="Pan Right">
            <icon>
               <image file="images:Button_ArrowRt.png"/>
            </icon>
         </action>
      </panRight>
      <panUp>
         <button>
            <iconSize width="18" height="18"/>
         </button>
         <action text="Pan Up" statusTip="Pan Up">
            <icon>
               <image file="images:Button_ArrowUp.png"/>
            </icon>
         </action>
      </panUp>
      <panDown>
         <button>
            <iconSize width="18" height="18"/>
         </button>
         <action text="Pan Down" statusTip="Pan Down">
            <icon>
               <image file="images:Button_ArrowDn.png"/>
            </icon>
         </action>
      </panDown>
   </NACanvasMapZoom>
   <NACanvasObject>
      <plugin-list>
         <plugin name="dmzQtPluginCanvasObjectBasic" unique="NACanvasObjectBasic"/>
      </plugin-list>
      <module>
         <canvas name="NACanvas"/>
      </module>
      <attribute>
         <position name="Alt_Position_Attribute"/>
      </attribute>
      <NACanvasObjectBasic>
         <defaults zValue="1" itemIgnoresTransformations="false"/>
         <module>
            <canvas name="NACanvas"/>
         </module>
      </NACanvasObjectBasic>
   </NACanvasObject>
   <NACanvasLink>
      <attribute>
         <position name="Alt_Position_Attribute"/>
      </attribute>
   </NACanvasLink>
   <NACanvasLayer>
      <dockWidget title="Networks"/>
      <module>
         <canvas name="NACanvas"/>
      </module>
      <!-- <widget>
         <stylesheet file="stylesheets:FileButton.qss"/>
      </widget> -->
      <addLayer>
         <action text="Add Network" statusTip="Add a new Newtork">
            <icon>
               <image file="images:Button_ZoomIn.png"/>
            </icon>
         </action>
      </addLayer>\
      <deleteLayer>
         <action text="Remove Network" statusTip="Remove selected Newtork" enabled="False">
            <icon>
               <image file="images:Button_ZoomOut.png"/>
            </icon>
         </action>
      </deleteLayer>\
   </NACanvasLayer>
   <na-properties>
      <edit-message name="EditObjectAttributesMessage"/>
      <object-widget-list>
         <widget type="line" name="Name:" attribute="NA_Node_Name"/>
         <widget
            type="scalar"
            name="Elimination Cost:"
            attribute="NA_Node_Elimination_Cost"
            prefix="$"
         />
         <widget
            type="scalar"
            name="Consequence:"
            attribute="NA_Node_Consequence"
            prefix="$"
         />
         <widget type="text" name="Description:" attribute="NA_Node_Description"/>
         <widget type="calc-label" name="Degrees:">
            <link name="Node_Link" super="true" sub="true"/>
         </widget>
      </object-widget-list>
      <link-widget-list>
         <widget type="line" name="Name:" attribute="NA_Node_Name"/>
         <widget
            type="scalar"
            name="Elimination Cost:"
            attribute="NA_Node_Elimination_Cost"
            prefix="$"
         />
         <widget
            type="scalar"
            name="Consequence:"
            attribute="NA_Node_Consequence"
            prefix="$"
         />
         <widget type="state" name="Flow:" attribute="NA_Link_Flow">
            <state label="both" name="NA_Flow_Forward|NA_Flow_Reverse" default="true"/>
            <state label="forward" name="NA_Flow_Forward"/>
            <state label="reverse" name="NA_Flow_Reverse"/>
         </widget>
         <widget type="text" name="Description:" attribute="NA_Node_Description"/>
         <widget type="link-label" name="From:" attribute="NA_Node_Name" super="true"/>
         <widget type="link-label" name="To:" attribute="NA_Node_Name" super="false"/>
      </link-widget-list>
   </na-properties>
   <dmzQtPluginButtonToChannel>
      <dockWidget title="Actions"/>
      <!-- <widget>
         <stylesheet file="stylesheets:ActionButton.qss"/>
      </widget> -->
      <!-- <defaultChannel name="NA_Default_Channel"/> -->
      <buttons>
         <toolButton channel="NA_Default_Channel" autoRaise="True">
            <button>
               <iconSize width="32" height="32"/>
            </button>
            <action text="Edit" statusTip="Edit" checkable="True" checked="True">
               <icon>
                  <image file="images:Button_Edit.png"/>
               </icon>
            </action>
         </toolButton>
         <toolButton channel="NA_Create_Object_Channel" autoRaise="True">
            <button>
               <iconSize width="32" height="32"/>
            </button>
            <action text="Add Node" statusTip="Add Node" checkable="True">
               <icon>
                  <image file="images:Button_NodeAdd.png"/>
               </icon>
            </action>
         </toolButton>
         <toolButton channel="NA_Link_Objects_Channel" autoRaise="True">
            <button>
               <iconSize width="32" height="32"/>
            </button>
            <action text="Add Link" statusTip="Add Link" checkable="True">
               <icon>
                  <image file="images:Button_LinkAdd.png"/>
               </icon>
            </action>
         </toolButton>
         <toolButton channel="NA_Destroy_Object_Channel" autoRaise="True">
            <button>
               <iconSize width="32" height="32"/>
            </button>
            <action text="Delete" statusTip="Delete" checkable="True">
               <icon>
                  <image file="images:Button_Delete.png"/>
               </icon>
            </action>
         </toolButton>
      </buttons>
   </dmzQtPluginButtonToChannel>
   <NACanvasArchive>
      <module>
         <canvas name="NACanvas"/>
      </module>
   </NACanvasArchive>
   <NAMapArchive>
      <module>
         <map name="NAMap"/>
      </module>
   </NAMapArchive>
   <dmzQtPluginMapProperties>
      <module>
         <map name="NAMap"/>
         <canvas name="NACanvas"/>
      </module>
   </dmzQtPluginMapProperties>
   <NARenderPickCanvas>
      <order x="0" y="2" z="1"/>
      <source name="NACanvas"/>
      <module>
         <canvas name="NACanvas"/>
         <pickConvert name="NAMap"/>
      </module>s
      <discover pickConvert="True"/>
   </NARenderPickCanvas>
   <NARenderPickMap>
      <order x="0" y="2" z="1"/>
      <source name="NAMap"/>
      <module>
         <canvas name="NAMap"/>
      </module>
   </NARenderPickMap>
   <dmzMBRAPluginCalculate>
      <!-- <widget>
         <stylesheet file="stylesheets:ActionButton.qss"/>
      </widget> -->
      <caclulateButton autoRaise="True">
         <button>
            <iconSize width="32" height="32"/>
         </button>
         <action text="Calculate" checkable="true">
            <icon>
               <image file="images:Button_Calc.png"/>
            </icon>
         </action>
      </caclulateButton>
   </dmzMBRAPluginCalculate>
   <dmzMBRAPluginNodeTable>
      <degrees>
         <link name="Node_Link" super="true" sub="true"/>
      </degrees>
      <dockWidget title="Network Table Data"/>
   </dmzMBRAPluginNodeTable>
   <dmzQtPluginCanvasAutoLink>
      <link>
         <type name="na_node"/>
         <type name="na_link_attribute"/>
      </link>
   </dmzQtPluginCanvasAutoLink>
<dmzQtPluginGraph>
   <start value="1"/>
   <ascending value="true"/>
   <set>
      <object-type name="na_node"/>
   </set>
   <attribute type="link" name="Node_Link"/>
   <bar>
      <stroke>
         <color blue="1"/>
      </stroke>
      <fill>
         <color red="0.5" green="0.5" blue="1"/>
      </fill>
   </bar>
</dmzQtPluginGraph>
<dmzQtPluginMapObjectPosition>
   <order x="0" y="2" z="1"/>
   <set>
      <object-type name="na_node"/>
      <object-type name="na_tool_link_node"/>
      <object-type name="na_link_attribute"/>
   </set>
   <attribute>
      <position name="Alt_Position_Attribute"/>
   </attribute>
   <source>
      <canvas name="NACanvas"/>
      <map name="NAMap"/>
   </source>
</dmzQtPluginMapObjectPosition>
<archive>
   <object type="canvas">
      <attributes>
         <position x="0.0" y="0.0" z="0.0"/>
      </attributes>
      <attributes name="Canvas_Zoom">
         <scalar value="0.000000"/>
      </attributes>
      <attributes name="Canvas_Base">
         <scalar value="0.000000"/>
      </attributes>
   </object>
</archive>
</dmz>

