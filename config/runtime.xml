   <object-type name="canvas"/>
   <object-type name="canvas_layer"/>
   <object-type name="layer" parent="canvas_layer">
      <dmzQtPluginCanvasLayer/>
   </object-type>
   <object-type name="root_layer" parent="layer">
      <dmzQtPluginCanvasLayer/>
   </object-type>
   <object-type name="na_tool_link_node">
      <NACanvasObject/>
   </object-type>
   <object-type name="na_link_attribute">
      <NACanvasObject/>
      <NACanvasObjectBasic>
         <items>
            <group name="overlay">
               <image file="NA_ResultsOverlayLinks.svg">
                  <translate x="0" y="-38"/>
                  </image>
               <text name="number">
                  <textColor>
                     <string value="white"/>
                  </textColor>
                  <enableBackground value="false"/>
                  <translate x="20" y="-20"/>
                  <scale x="2.0" y="2.0"/>
               </text>
            </group>
         </items>
         <text>
            <item name="number" attribute="NA_Node_Rank"/>
         </text>
         <switch>
            <state name="NA_Node_Overlay" group="overlay">
               <item name="overlay"/>
            </state>
         </switch>
      </NACanvasObjectBasic>
   </object-type>
   <object-type name="na_node">
      <NACanvasObject/>
      <NACanvasObjectBasic>
         <items>
            <image name="node" file="NA_Node.svg">
               <translate x="-10" y="-10"/>
            </image>
            <group name="overlay">
               <image file="NA_ResultsOverlay.svg">
                  <translate x="-19.5" y="-37.5"/>
               </image>
               <text name="number">
                  <textColor>
                     <string value="white"/>
                  </textColor>
                  <enableBackground value="false"/>
                  <translate x="20" y="-20"/>
                  <scale x="2.0" y="2.0"/>
               </text>
            </group>
            <text name="name">
               <textColor>
                  <string value="white"/>
                  <alpha value="0.75"/>
               </textColor>
               <outlineColor>
                  <string value="black"/>
                  <alpha value="0.75"/>
               </outlineColor>
               <backgroundColor>
                  <string value="black"/>
                  <alpha value="0.75"/>
               </backgroundColor>
               <translate x="50" y="0"/>
            </text>
         </items>
         <text>
            <item name="number" attribute="NA_Node_Rank"/>
            <item name="name" attribute="NA_Node_Name"/>
         </text>
         <switch>
            <state>
               <item name="node"/>
               <item name="name"/>
            </state>
            <state name="NA_Node_Overlay" group="overlay">
               <item name="overlay"/>
            </state>
            <!-- <flag attribute="NA_Node_Overlay">
               <item name="overlay"/>
            </flag> -->
         </switch>
      </NACanvasObjectBasic>
   </object-type>
   <object-type name="ft_node"/>
   <object-type name="ft_component" parent="ft_node">
      <FTCanvasObject/>
      <FTCanvasObjectBasic>
         <items>
            <group name="component">
               <image file="FT_ComponentBox.svg">
                  <translate x="-70" y="-16"/>
               </image>
               <text name="name">
                  <textColor>
                     <string value="black"/>
                  </textColor>
                  <alignment value="center"/>
                  <enableBackground value="false"/>
               </text>
            </group>
         </items>
         <text>
            <item name="name" attribute="FT_Name"/>
         </text>
         <switch>
            <state>
               <item name="component"/>
            </state>
         </switch>
      </FTCanvasObjectBasic>
   </object-type>
   <object-type name="ft_component_root" parent="ft_component"/>
