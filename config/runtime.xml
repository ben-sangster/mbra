   <object-type name="ft_logic" parent="ft_node">
      <FTCanvasObject/>
       <FTCanvasObjectBasic>
          <items>
             <group name="and">
                <image resource="ft-logic-and">
                   <translate x="-20" y="-20"/>
                </image>
                <text>
                   <text value="AND"/>
                   <textColor>
                      <string value="black"/>
                   </textColor>
                   <alignment value="center"/>
                   <enableBackground value="false"/>
                </text>
             </group>
             <group name="or">
                <image resource="ft-logic-or">
                   <translate x="-20" y="-20"/>
                </image>
                <text>
                   <text value="OR"/>
                   <textColor>
                      <string value="black"/>
                   </textColor>
                   <alignment value="center"/>
                   <enableBackground value="false"/>
                </text>
             </group>
             <group name="xor">
                <image resource="ft-logic-xor">
                   <translate x="-20" y="-20"/>
                </image>
                <text>
                   <text value="XOR"/>
                   <textColor>
                      <string value="black"/>
                   </textColor>
                   <alignment value="center"/>
                   <enableBackground value="false"/>
                </text>
             </group>
          </items>
          <switch>
             <state group="logic" name="FT_Logic_And"><item name="and"/></state>
             <state group="logic" name="FT_Logic_Or"><item name="or"/></state>
             <state group="logic" name="FT_Logic_XOr"><item name="xor"/></state>
          </switch>
