   <object-type name="canvas"/>
   <object-type name="canvas_layer"/>
   <object-type name="layer" parent="canvas_layer">
      <dmzQtPluginCanvasLayer/>
   </object-type>
   <object-type name="root_layer" parent="layer">
      <dmzQtPluginCanvasLayer/>
   </object-type>
   <object-type name="na_simulator"/>
   <object-type name="na_objective_bar"/>
   <object-type name="na_cascade_bar"/>
   <object-type name="na_tool_link_node">
      <NACanvasObject/>
   </object-type>
   <object-type name="na_link_attribute">
      <NACanvasObject/>
      <NACanvasObjectBasic>
         <items>
            <group name ="flag">
               <image name="node-flag" resource="node-flag">
                  <translate x="-4" y="-38"/>
               </image>
            </group>
            <group name="overlay">
               <image resource="link-overlay">
                  <translate x="-38" y="0"/>
<!--
                  <translate x="0" y="-38"/>
-->
                  </image>
               <text name="number">
                  <textColor>
                     <string value="white"/>
                  </textColor>
                  <enableBackground value="false"/>
                  <translate x="-27" y="20"/>
<!--
                  <translate x="20" y="-20"/>
-->
                  <scale x="2.0" y="2.0"/>
               </text>
            </group>
         </items>
         <text>
            <item name="number" attribute="NA_Node_Rank"/>
         </text>
         <switch>
            <state name="NA_Node_Flagged" group="flag">
               <item name="node-flag"/>
            </state>
            <state name="NA_Node_Overlay" group="overlay">
               <item name="overlay"/>
            </state>
         </switch>
      </NACanvasObjectBasic>
   </object-type>
   <object-type name="na_node">
      <NACanvasObject/>
      <NACanvasObjectBasic>
         <items>
            <group name="overlay">
               <image resource="node-overlay">
                  <translate x="-37.5" y="-19.0"/>
               </image>
               <text name="number">
                  <textColor>
                     <string value="white"/>
                  </textColor>
                  <enableBackground value="false"/>
                  <translate x="-27" y="20"/>
                  <scale x="2.0" y="2.0"/>
               </text>
            </group>
            <group name ="flag">
               <image name="node-flag" resource="node-flag">
                  <translate x="-1" y="-34"/>
               </image>
            </group>
            <group name="base node">
               <image name="node" resource="node">
                  <translate x="-10" y="-10"/>
               </image>
               <image name="node-highlight" resource="node-highlight">
                  <translate x="-10" y="-10"/>
               </image>
               <text name="name">
                  <textColor>
                     <string value="white"/>
                     <alpha value="0.75"/>
                  </textColor>
                  <outlineColor>
                     <string value="black"/>
                     <alpha value="0.75"/>
                  </outlineColor>
                  <backgroundColor>
                     <string value="black"/>
                     <alpha value="0.75"/>
                  </backgroundColor>
                  <translate x="30" y="0"/>
               </text>
               <text name="name-highlight">
                  <textColor>
                     <string value="yellow"/>
                     <alpha value="1.0"/>
                  </textColor>
                  <outlineColor>
                     <string value="black"/>
                     <alpha value="1.0"/>
                  </outlineColor>
                  <backgroundColor>
                     <string value="black"/>
                     <alpha value="0.8"/>
                  </backgroundColor>
                  <translate x="30" y="0"/>
               </text>
            </group>
         </items>
         <text>
            <item name="number" attribute="NA_Node_Rank"/>
            <item name="name" attribute="NA_Node_Label"/>
            <item name="name-highlight" attribute="NA_Node_Label"/>
         </text>
         <switch>
            <state group="base node">
               <item name="node"/>
               <item name="name"/>
            </state>
            <state name="NA_Node_Highlight" group="base node">
               <item name="node-highlight"/>
               <item name="name-highlight"/>
            </state>
            <state name="NA_Node_Overlay" group="overlay">
               <item name="overlay"/>
            </state>
            <state name="NA_Node_Flagged" group="flag">
               <item name="node-flag"/>
            </state>
         </switch>
      </NACanvasObjectBasic>
   </object-type>
   <object-type name="ft_clip_board"/>
   <object-type name="ft_node"/>
   <object-type name="ft_component" parent="ft_node">
      <FTCanvasObject/>
      <FTCanvasObjectBasic>
         <items>
            <group name="component">
               <image resource="ft-component">
                  <translate x="-70" y="-16"/>
               </image>
               <text name="name">
                  <textColor>
                     <string value="black"/>
                  </textColor>
                  <alignment value="center"/>
                  <max-length value="12"/>
                  <enableBackground value="false"/>
               </text>
            </group>
         </items>
         <text>
            <item name="name" attribute="FT_Name"/>
         </text>
         <switch>
            <state>
               <item name="component"/>
            </state>
         </switch>
      </FTCanvasObjectBasic>
   </object-type>
   <object-type name="ft_component_root" parent="ft_component"/>
